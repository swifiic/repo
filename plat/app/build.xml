<project name="SWiFiIC-plat-app-soa" default="Apps" basedir=".">
<target name="Apps" description="Build Operator App">
    <ant dir="soa" />
    <ant dir="suta" />
    <copy file="suta/andi/bin/SUTA-debug.apk" todir="../../dist" />
    <copy file="soa/andi/bin/SOA-debug.apk" todir="../../dist" />
